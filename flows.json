    },
    {
        "id": "f472fb257ea5d09a",
        "type": "function",
        "z": "075f686498467449",
        "name": "function 3",
        "func": "let hum = parseFloat(msg.payload);\nlet warning = \"\";\nif (hum < 60) {\n    warning = \"⚠️ Humidity too low!\";\n} else if (hum > 80) {\n    warning = \"⚠️ Humidity too high!\";\n} \nmsg.payload = warning;\nreturn msg;\n",
