            ]
        ]
    },
    {
        "id": "310586b465f915d7",
        "type": "function",
        "z": "075f686498467449",
        "name": "function 2",
        "func": "let temp = parseFloat(msg.payload);\nlet warning = \"\";\nif (temp < 20) {\n    warning = \"⚠️ Temperature too low!\";\n} else if (temp > 35) {\n    warning = \"⚠️ Temperature too high!\";\n} \nmsg.payload = warning;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 460,
